{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<div><h3>{{'Are you sure you want to delete this Multiparameter?'}}</h3></div>
<br>
<form id="appform" method="post" action="{{deleteurl}}">
    <table  border="0" cellspacing="1" cellpadding="2">
       <table  border="0" cellspacing="1" cellpadding="2">
           <tr>
               <td><label class="control-label">{{'System Parameter'}}</label></td>
               <td>{{obj.getsysparamselect('',sysparamid)}}</td>
           </tr>
           <tr>
               <td><label class="control-label">{{'Value'}}</label></td>
               <td><textarea  id ="value"  name="value"  rows="3" maxlength="900" readonly>{{value}}</textarea></td>
               <td><label id ="valueerror" name="valueerror"></label></td>
           </tr>
           <tr>
               <td><label class="control-label">{{'Description'}}</label></td>
               <td><input name="valuedesc" type="text" id="valuedesc" maxlength="100" class="span6 typeahead" value="{{valuedesc}}" readonly></td>
           </tr>

    </table>
    <br>
    <div class="options btn-group">
        <input  id ="deletebutton" class="btn btn-primary" type="submit" value="Delete" />
        <input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
    </div>
    <input type="hidden" id="id" name="id"  value="{{id}}">
</form>
{% endblock content %}