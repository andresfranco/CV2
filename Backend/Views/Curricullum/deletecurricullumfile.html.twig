{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block content %}
  {% block breadcrumb %}
     <ol class="breadcrumb">
     <li><a href="{{link}}">{{option|raw}}</a></li>
     <li><a href="{{linkroute}}" class="route">{{route|raw}}</a></li>
     </ol>
 {% endblock breadcrumb%}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this File?'}}</h4></div>
<br>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" enctype="multipart/form-data" action="{{deleteurl}}" class="form-horizontal" role="form">
    
    <div class="form-group">
            <label class="control-label col-sm-2">{{'Curricullum'}}</label>
            <div class="col-sm-4">
            <input id ="name" name="cvname" type="text" id="language" value="{{cvname|raw}}" class="form-control" readonly>
            </div>
    </div>
    <div class="form-group">
            <label class="control-label col-sm-2">{{'Language'}}</label>
            <div class="col-sm-4">
            {{globalobj.getlanguageselect('class="form-control" Disabled',languagecode)}}
            </div>
    </div>
   
     <div class="form-group">
            <label class="control-label col-sm-2">{{'Curriculum File'}}</label>
            <div class="col-sm-4">
            <input id="filename" name="filename" class="form-control" type="text" readonly value="{{filename|raw}}" Disabled/>
            </div>
    </div>
    <div class="form-group">
        <br><br>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">Cancel</a>
        </div>
    </div>
</form>
</div>

{% endblock content%}

