{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<div><h3>{{'Are you sure you want to delete this Curricullum?'}}</h3></div>
<br>
<form id="appform" method="post" action="{{deleteurl}}">
    <table>
       
        <tr>
            <td><label class="control-label">{{'Name'}}</label></td>
            <td><input id ="name" name="name" type="text" id="language" class="span6 typeahead" value="{{name}}" readonly></td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Main Text'}}</label></td>
            <td><textarea  id="maintext" name="maintext" rows="3" readonly>{{maintext}}</textarea></td>

        </tr>
        <tr>
            <td><label class="control-label">{{'About Me'}}</label></td>
            <td><textarea  name="aboutme" id="aboutme" rows="3" readonly>{{aboutme}}</textarea></td>

        </tr>
        <tr>
            <td><label class="control-label">{{'Contact Details'}}</label></td>
            <td><textarea  name="contactdetails" id="contactdetails" rows="3" readonly>{{contactdetails}}</textarea></td>

        </tr>
        <tr>
            <td><label class="control-label">{{'Main Skills'}}</label></td>
            <td><textarea name="mainskills" id="mainskills" rows="3" readonly>{{mainskills}}</textarea></td>

        </tr>


    </table>
    <br>
    <div class="options btn-group">
        <input  id ="deletebutton" class="btn btn-primary" type="submit" value="Delete" />
        <a href="{{listurl}}" id ="cancelbutton" class=" btn input-small">{{'Cancel'}}</a>
    </div>
</form>
{% endblock content%}