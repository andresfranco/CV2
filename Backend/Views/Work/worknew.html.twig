{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<script src="{{templatepath~'/Views/Work/validatework.js'}}"></script>
<label class="error">{{errormessage|raw}}</label><br>
<form id="appform" method="post" action="{{selfurl}}">
    <table width="400" border="0" cellspacing="1" cellpadding="2">
        <tr>
            <td ><label class="control-label">{{'Curricullum'}}</label></td>
            <td>{{globalobj.getcurricullumlist('','')}}</td>
        </tr>
       <tr>
            <td><label class="control-label">{{'Company'}}</label></td>
            <td><input size="60" id ="company" name="company" type="text" id="language" class="span6 typeahead" value="{{company|raw}}"></td>   
        </tr>
        <tr>
            <td><label class="control-label">{{'Position'}}</label></td>
            <td><input id ="position" name="position" type="text" id="language" class="span6 typeahead" value="{{position|raw}}"></td>
        </tr>
        <tr>
            <td><label class="control-label">{{'From'}}</label></td>
            <td><input id ="from" name="from" type="text" id="language" class="span6 typeahead" value="{{from|raw}}"></td>
        </tr>
        <tr>
            <td><label class="control-label">{{'To'}}</label></td>
            <td><input id ="to" name="to" type="text" id="language" class="span6 typeahead" value="{{to|raw}}"></td>
        </tr>
       
    </table>
    <br>
    <div class="options btn-group">
        <input  id ="deletebutton" class="btn btn-primary" type="submit" value="Save" />
        <input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
    </div>
</form>
{% endblock content%}
