{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block content %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this Work?'}}</h4></div>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{deleteurl}}" class="form-horizontal" role="form">
        <div class="form-group">
            <label class="control-label col-sm-2">{{'Curricullum'}}</label>
            <div class="col-sm-3">
            {{globalobj.getcurricullumlist('class="form-control" Disabled',curricullumid)}}
            </div>
         </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Company'}}</label>
              <div class="col-sm-3">
              <input size="60" id ="company" name="company" type="text" id="language" class="form-control" value="{{company|raw}}" Disabled>
              </div>      
        </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Position'}}</label>
              <div class="col-sm-3">
              <input id ="position" name="position" type="text" id="language" class="form-control" value="{{position|raw}}" Disabled>   
              </div>      
        </div> 
        <div class="form-group">
             <label class="control-label col-sm-2">{{'From'}}</label>
              <div class="col-sm-3">
              <input id ="from" name="from" type="text" id="language" class="form-control" value="{{from|raw}}" Disabled>
              </div> 
        </div>
         <div class="form-group">
             <label class="control-label col-sm-2">{{'To'}}</label>
              <div class="col-sm-3">
              <input id ="to" name="to" type="text" id="language" class="form-control" value="{{to|raw}}" Disabled>
              </div> 
        </div> 
        <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">{{'Cancel'}}</a>
        </div>
    </div>
</form>
</div>

{% endblock content%}