{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<script src="{{templatepath~'/Views/Project/disableprojectfields.js'}}"></script>
<div><h3>{{'Are you sure you want to delete this Project?'}}</h3></div>
<br>
<form id="appform" method="post" action="{{deleteurl}}">
<table width="400" border="0" cellspacing="1" cellpadding="2">
<tr>
            <td ><label class="control-label">{{'Curricullum'}}</label></td>
            <td>{{globalobj.getcurricullumlist('',curricullumid)}}</td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Name'}}</label></td>
            <td><input id ="name" name="name" type="text" id="language" class="span6 typeahead" value="{{name|raw}}" readonly></td>   
        </tr>
        <tr>
            <td><label class="control-label">{{'Description'}}</label></td>
            <td><input id ="description" name="description" type="text" id="language" class="span6 typeahead" value="{{description|raw}}" readonly></td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Link'}}</label></td>
            <td><input id ="link" name="link" type="text" id="language" class="span6 typeahead" value="{{linkproject|raw}}" readonly></td>
        </tr>

</table>
<br>
<div class="options btn-group">
<input  id ="deletebutton" class="btn btn-primary" type="submit" value="Delete" />
<input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
</div>
<input type="hidden" id="id" name="id"  value="{{id}}">
</form>
{% endblock content%}
