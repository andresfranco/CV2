{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
{% block content %}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this User Role?'}}</h4></div>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{deleteurl}}" class="form-horizontal" role="form">
        <div class="form-group">
            <label class="control-label col-sm-2">{{'Username'}}</label>
            <div class="col-sm-3">
             {{obj.get_user_select(userid,'class="form-control" Disabled')}}
            </div>
         </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Role'}}</label>
              <div class="col-sm-3">
               {{obj.get_role_select(roleid,'class="form-control" Disabled')}}
              </div> 
        </div>     
        <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">Cancel</a>
        </div>
        </div>
</form>
</div>

{% endblock content%}
