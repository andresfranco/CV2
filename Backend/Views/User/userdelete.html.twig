{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<div><h3>{{'Are you sure you want to delete this User?'}}</h3></div>
<br>
<form id="appform" method="post" action="{{deleteurl}}">
    <table width="400" border="0" cellspacing="1" cellpadding="2">
        <tr>
            <td width="100"><label class="control-label">{{'Username'}}</label></td>
            <td><input name="username" type="text" id="username" class="span6 typeahead" value="{{username}}" readonly>
       <span class="error"></span></td>
        </tr>
        <tr>
            <td width="100"><label class="control-label">{{'Email'}}</label></td>
            <td><input name="email" type="text" id="email" class="span6 typeahead" value="{{email}}" readonly></td>
        </tr>
    </table>
    <br>
    <div class="options btn-group">
        <input  id ="deletebutton" class="btn btn-primary" type="submit" value="Delete" />
        <input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
    </div>
     <input type="hidden" id="id" name="id"  value="{{id}}">
</form>
{% endblock content %}