{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<script src="{{templatepath~'/Views/Skill/validateskill.js'}}"></script>
<label class="error">{{errormessage|raw}}</label><br>
<form id="appform" method="post" action="{{selfurl}}">
    <table width="400" border="0" cellspacing="1" cellpadding="2">
        <tr>
            <td ><label class="control-label">{{'Curricullum'}}</label></td>
            <td>{{globalobj.getcurricullumlist('',curricullumid)}}</td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Type'}}</label></td>
            <td>{{globalobj. getmultiparamselect('skilltype',type,'','Please select a Type','type','type')}}</td>   
        </tr>
        <tr>
            <td><label class="control-label">{{'Skill'}}</label></td>
            <td><input id ="skill" name="skill" type="text" id="language" class="span6 typeahead" value="{{skill|raw}}"></td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Percentage'}}</label></td>
            <td><input id ="percentage" name="percentage" type="text" id="language" class="span6 typeahead" value="{{percentage|raw}}"></td>
        </tr>
       
    </table>
    <br>
    <div class="options btn-group">
        <input  id ="deletebutton" class="btn btn-primary" type="submit" value="Save" />
        <input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
    </div>
</form>
{% endblock content%}
