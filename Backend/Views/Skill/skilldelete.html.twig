{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<script src="{{templatepath~'/Views/Skill/disableskillfields.js'}}"></script>
<div><h3>{{'Are you sure you want to delete this Skill?'}}</h3></div>
<br>
<form id="appform" method="post" action="{{deleteurl}}">
<table width="400" border="0" cellspacing="1" cellpadding="2">
<tr>
            <td ><label class="control-label">{{'Curricullum'}}</label></td>
            <td>{{globalobj.getcurricullumlist('',curricullumid)}}</td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Type'}}</label></td>
            <td>{{globalobj. getmultiparamselect('skilltype',type,'','Please select a Type','type','type')}}</td>   
        </tr>
        <tr>
            <td><label class="control-label">{{'Skill'}}</label></td>
            <td><input id ="skill" name="skill" type="text" id="language" class="span6 typeahead" value="{{skill|raw}}" readonly></td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Percentage'}}</label></td>
            <td><input id ="percentage" name="percentage" type="text" id="language" class="span6 typeahead" value="{{percentage|raw}}" readonly></td>
        </tr>

</table>
<br>
<div class="options btn-group">
<input  id ="deletebutton" class="btn btn-primary" type="submit" value="Delete" />
<input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
</div>
<input type="hidden" id="id" name="id"  value="{{id}}">
</form>
{% endblock content%}
