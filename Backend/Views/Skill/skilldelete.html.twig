{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block content %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this Skill?'}}</h4></div>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{deleteurl}}" class="form-horizontal" role="form">
        <div class="form-group">
            <label class="control-label col-sm-2">{{'Curricullum'}}</label>
            <div class="col-sm-3">
            {{globalobj.getcurricullumlist('class="form-control" Disabled',skill.curricullumid)}}
            </div>
         </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Type'}}</label>
              <div class="col-sm-3">
              {{globalobj. getmultiparamselect('skilltype',skill.type,'class="form-control" Disabled','Please select a Type','type','type')}}
              </div>      
        </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Skill'}}</label>
              <div class="col-sm-3">
              <input id ="skill" name="skill" type="text" id="language" class="form-control"  value="{{skill.skill|raw}}" Disabled>    
              </div>      
        </div> 
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Percentage'}}</label>
              <div class="col-sm-3">
             <input id ="percentage" name="percentage" type="text" id="language" class="form-control" value="{{skill.percentage|raw}}" Disabled>
              </div> 
        </div> 
         <div class="form-group">
              <div class="checkbox" style="padding-left:17.5%;">
              <label><input type="checkbox" name="active" id="active" {% if skill.active==1 %} checked {% endif %} Disabled>{{'Active'}}</label>
              </div>   
        </div>
        <div class="form-group">
            <label class="control-label col-sm-2">{{'Description'}}</label>
            <div class="col-sm-8">
               <textarea id ="description" name="description"  rows="3" Disabled>{{skill.description|raw}}</textarea> 
          </div>
        </div>
        <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">{{'Cancel'}}</a>
        </div>
    </div>
</form>
</div>

{% endblock content%}