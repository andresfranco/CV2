{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block content %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this Language?'}}</h4></div>
<br>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{deleteurl}}" class="form-horizontal" role="form">
         <div class="form-group">
            <label class="control-label col-sm-2">{{'Code'}}</label>
            <div class="col-sm-3">
            <input name="code" type="text" id="code" value="{{code}}" class="form-control" disabled>
            </div>
         </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Language'}}</label>
              <div class="col-sm-3">
              <input name="language" type="text" id="language"  value="{{language}}" class="form-control" disabled>
              </div>      
        </div>
      
        <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">{{'Cancel'}}</a>
        </div>
    </div>
</form>
</div>
{% endblock content%}