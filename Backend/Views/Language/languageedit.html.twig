{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block javascript%} 
{{parent()}}
<script src="{{templatepath ~'/Views/Language/validatelanguage.js'}}"></script>
{% endblock javascript%} 
{% block content %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
<label class="col-sm-offset-2 col-sm-8 error">{{errormessage|raw}}</label>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{updateurl}}" class="form-horizontal" role="form">
         <div class="form-group">
            <label class="control-label col-sm-2">{{'Code'}}</label>
            <div class="col-sm-3">
            <input name="code" type="text" id="code" value="{{code}}" class="form-control">
            </div>
         </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Language'}}</label>
              <div class="col-sm-3">
              <input name="language" type="text" id="language"  value="{{language}}" class="form-control">
              </div>      
        </div>
      
        <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="savebutton" class="btn btn-primary" type="submit" value="Save"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">Cancel</a>
        </div>
    </div>
</form>
</div>
{% endblock content%}