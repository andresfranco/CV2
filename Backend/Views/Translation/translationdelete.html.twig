{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{%block javascript%}
{{parent()}}
<script src="{{templatepath~'/Views/Translation/disabletranslationfields.js'}}"></script>
{% endblock javascript%}
{% block content %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this Translation?'}}</h4></div>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{deleteurl}}" class="form-horizontal" role="form">
    <div class="form-group">
            <label class="control-label col-sm-2">{{'Language'}}</label>
            <div class="col-sm-4">{{globalobj.getlanguageselect('class="form-control"',languagecode)}}</div>
        </div>
    <div class="form-group">
            <label class="control-label col-sm-2">{{'Object Code'}}</label>
            <div class="col-sm-4">{{globalobj.getmultiparambycode('objcode',objectcode,'class="form-control"')}}</div>
    </div>
        <div class="form-group">
            <label class="control-label col-sm-2">{{'Parent'}}</label>
            <div class="col-sm-4">{{db.getparent(globalobj,objectcode,parentid,'class="form-control"')}}</div>
        </div>
       <div class="form-group">
            <label class="control-label col-sm-2">{{'Object ID'}}</label>
            <div class="col-sm-4">{{db.getobject(globalobj,objectcode,objectid,'class="form-control"')}}</div>
        </div>
    
       <div class="form-group">
            <label class="control-label col-sm-2">{{'Field'}}</label>
            <div class="col-sm-4">{{db.getfields(globalobj,objectcode,field,'class="form-control"')}}</div>
        </div>
       <div class="form-group">
            <label class="control-label col-sm-2">{{'Content'}}</label>
            <div class="col-sm-4">
               <textarea id ="translationcontent" name="translationcontent"  rows="3" class="form-control" disabled>{{content|raw}}</textarea> 
          </div>
            <div class="col-sm-offset-2 col-sm-8">    
            <label id ="contenterror" name="contenterror"></label>    
            </div>
        </div>
       
   
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">Cancel</a>
        </div>
    </div>
</form>
</div>
{% endblock content%}
