{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<script src="{{templatepath~'/Views/Translation/disabletranslationfields.js'}}"></script>
<div><h3>{{'Are you sure you want to delete this Translation?'}}</h3></div>
<br>
<form id="appform" method="post" action="{{deleteurl}}">
<table>
<tr>
<td><label class="control-label">{{'Object Code'}}</label></td>
<td>{{globalobj.getmultiparambycode('objcode',objectcode,'')}}</td>
</tr>

<tr>
<td><label class="control-label">{{'Parent'}}</label></td>

<td>{{db.getparent(globalobj,objectcode,parentid)}}
</td>

</tr>
<tr>
<td><label class="control-label">{{'Object ID'}}</label></td>

<td>{{db.getobject(globalobj,objectcode,objectid)}}
</td>

</tr>
<tr>
<td><label class="control-label">{{'Language'}}</label></td>
<td>{{globalobj.getlanguageselect('',languagecode)}}</td>

</tr>
<tr>
<td><label class="control-label">{{'Field'}}</label></td>
<td>{{db.getfields(globalobj,objectcode,field)}}
</td>

</tr>
<tr>
<td><label class="control-label">{{'Content'}}</label></td>
<td><textarea  rows="3" disabled>{{content}}</textarea></td>

</tr>

</table>
<br>
<div class="options btn-group">
<input  id ="deletebutton" class="btn btn-primary" type="submit" value="Delete" />
 <a href="{{listurl}}" id ="cancelbutton" class=" btn input-small">{{'Cancel'}}</a>
</div>
</form>
{% endblock content%}