{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<script src="{{templatepath~'/Views/Projecttag/disableprojecttagfields.js'}}"></script>
<div><h3>{{'Are you sure you want to delete this Project?'}}</h3></div>
<br>
<form id="appform" method="post" action="{{deleteurl}}">
<table width="400" border="0" cellspacing="1" cellpadding="2">
        <tr>
            <td ><label class="control-label">{{'Project'}}</label></td>
            <td>{{obj.getprojectselectbyid('',projectid)}}</td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Tag Name'}}</label></td>
            <td><input id ="tagname" name="tagname" type="text"  class="span6 typeahead" value="{{tagname|raw}}" readonly></td>   
        </tr>
</table>
<br>
<div class="options btn-group">
<input  id ="deletebutton" class="btn btn-primary" type="submit" value="Delete" />
<input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
</div>
</form>
{% endblock content%}
