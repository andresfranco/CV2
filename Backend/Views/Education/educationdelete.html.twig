{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block content %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this Education?'}}</h4></div>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{deleteurl}}" class="form-horizontal" role="form">
        <div class="form-group">
            <label class="control-label col-sm-2">{{'Curricullum'}}</label>
            <div class="col-sm-3">
            {{globalobj.getcurricullumlist('class="form-control" Disabled',curricullumid)}}
            </div>
         </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Institution'}}</label>
              <div class="col-sm-3">
             <input size="60" id ="institution" name="institution" type="text" id="language" class="form-control" value="{{institution|raw}}" Disabled>
              </div>      
        </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Degree'}}</label>
              <div class="col-sm-3">
              <input id ="degree" name="degree" type="text" id="language" class="form-control" value="{{degree|raw}}" Disabled>
              </div>      
        </div> 
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Date'}}</label>
              <div class="col-sm-3">
              <input id ="date" name="date" type="text" id="language" class="form-control" value="{{date|raw}}" Disabled>
              </div>      
        </div> 
        <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">{{'Cancel'}}</a>
        </div>
    </div>
</form>
</div>
{% endblock content%}
