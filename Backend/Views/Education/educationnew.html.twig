{% extends "Templates/Masterpage/masterbackend.html.twig" %}
{% block content %}
<script src="{{templatepath~'/Views/Education/validateeducation.js'}}"></script>
<label class="error">{{errormessage|raw}}</label><br>
<form id="appform" method="post" action="{{selfurl}}">
    <table width="400" border="0" cellspacing="1" cellpadding="2">
        <tr>
            <td ><label class="control-label">{{'Curricullum'}}</label></td>
            <td>{{globalobj.getcurricullumlist('','')}}</td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Institution'}}</label></td>
            <td><input size="60" id ="institution" name="institution" type="text" id="language" class="span6 typeahead" value="{{institution|raw}}"></td>   
        </tr>
        <tr>
            <td><label class="control-label">{{'Degree'}}</label></td>
            <td><input id ="degree" name="degree" type="text" id="language" class="span6 typeahead" value="{{degree|raw}}"></td>
        </tr>
        <tr>
            <td><label class="control-label">{{'Date'}}</label></td>
            <td><input id ="date" name="date" type="text" id="language" class="span6 typeahead" value="{{date|raw}}"></td>
        </tr>
       
    </table>
    <br>
    <div class="options btn-group">
        <input  id ="deletebutton" class="btn btn-primary" type="submit" value="Save" />
        <input onClick="window.location.href='{{listurl}}'"id ="cancelbutton" class=" btn input-small"  value="Cancel" />
    </div>
</form>
{% endblock content%}
