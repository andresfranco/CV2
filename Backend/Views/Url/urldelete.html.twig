{% extends "Templates/Masterpage/masteradmin.html.twig" %}
{% block content %}
{% block breadcrumb %}
{{parent()}}
{% endblock breadcrumb%}
<div class="col-sm-offset-2 col-sm-8 deletequestion"><h4>{{'Are you sure you want to delete this Url?'}}</h4></div>
<div id="formcontentgrid" class="col-sm-12">
<form id="appform" method="post" action="{{deleteurl}}" class="form-horizontal" role="form">
        <div class="form-group">
            <label class="control-label col-sm-2">{{'Curricullum'}}</label>
            <div class="col-sm-3">
            {{globalobj.getcurricullumlist('class="form-control" Disabled',curricullumid)}}
            </div>
         </div>
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Type'}}</label>
              <div class="col-sm-3">
              {{globalobj.getmultiparamselect('urltype',type,'class="form-control" Disabled','Select a Type','type','type')}}
              </div>      
        </div>
        <div class="form-group">
             <label class="control-label col-sm-2" Disabled>{{'Name'}}</label>
              <div class="col-sm-3">
              
              <input id ="name" name="name" type="text" id="language" class="form-control" value="{{name|raw}}" Disabled>    
              </div>      
        </div> 
        <div class="form-group">
             <label class="control-label col-sm-2">{{'Link'}}</label>
              <div class="col-sm-3">
              <textarea id ="link"  name="link" maxlength="900" class="form-control" Disabled>{{linkurl}}</textarea>
              </div> 
             <div class="col-sm-offset-2 col-sm-8">
              <label id ="linkerror" name="linkerror"></label>
              </div>
        </div> 
        <div class="form-group">
        <div class="col-sm-offset-2 col-sm-8">
        <input  id="deletebutton" class="btn btn-primary" type="submit" value="Delete"/>
        <a href="{{listurl}}" id ="cancelbutton" class="btn btn-primary cancel">Cancel</a>
        </div>
    </div>
</form>
</div>

{% endblock content%}
